    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_542049_ideas.glideRecordToObject</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>This script converts a glide record object into an object that JSON can convert into a string.</description>
        <name>glideRecordToObject</name>
        <script><![CDATA[function glideRecordToObject(gr) {
  var obj = {};

  for (var prop in gr) {
    if (gr[prop]){
      obj[prop] = gr.getValue(prop);
    }
  }

  return obj;
}]]></script>
